XTerm*termName: xterm-256color
! utf8-Einstellungen des Systems übernehmen!
XTerm*locale: true
XTerm*faceNameDoublesize: WenQuanYi Bitmap Song:antialias=true
!XTerm*foreground: rgb:BB/BB/BB
!XTerm*background: rgb:22/22/22
!#002B36
XTerm*foreground: rgb:00/88/00
XTerm*background: rgb:00/2B/36
!XTerm*faceName: Terminus
XTerm*faceName: DejaVu Sans Mono Book
XTerm*faceSize: 15
!XTerm*initialFont: 2
!
!folgendes damit emacs ALT benutzen kann
XTerm*metaSendsEscape: true
Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.hinting:true
Xft.hintstyle: hintfull
Xft.rgba: rgb
*customization: -color
!für FontForge, hoffentlich sieht die Schrift jetzt besser aus..
Gdraw.ScreenWidthPixels: 1920 
!Gdraw.ScreenWidthCentimeters: 27.3


! terminal colors ------------------------------------------------------------

! tangoesque scheme
*background: #111111
*foreground: #babdb6
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff

